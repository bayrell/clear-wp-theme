{
    "name": "App",
    "assets": [
        "Components\/Blocks\/CSS.bay",
        "Components\/IndexPage\/IndexPage.bay",
        "Components\/IndexPage\/IndexPageModel.bay",
        "Components\/Layout\/DefaultLayoutModel.bay",
        "Components\/NotFoundPage\/NotFoundPage.bay",
        "Components\/NotFoundPage\/NotFoundPageModel.bay",
        "ModuleDescription.bay"
    ],
    "widgets": [
        {
            "name": "App.Components.IndexPage.IndexPageModel",
            "kind": "page"
        },
        {
            "name": "App.Components.NotFoundPage.NotFoundPageModel",
            "kind": "page"
        }
