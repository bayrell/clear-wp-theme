{
    "name": "Clear theme",
    "description": "Clear theme",
    "type": "wordpress",
    "languages": [
        "php",
        "es6"
    ],
    "entry_points": [],
    "modules": [
        {
            "src": "./",
            "type": "lib"
        },
        {
            "src": "Admin",
            "type": "lib"
        }
    ],
    "assets": [
        {
            "type": "js",
            "dest": "Assets/app.js",
            "modules": [
                "@app"
            ]
        },
        {
            "type": "js",
            "dest": "Assets/admin.js",
            "modules": [
                "@admin"
            ]
        },
        {
            "type": "js",
            "dest": "Assets/constructor.js",
            "modules": [
                "@constructor"
            ]
        },
        {
            "type": "js",
            "dest": "Assets/test.js",
            "modules": [
                "@test"
            ]
        }
    ],
    "exclude": [],
    "domains": []
}